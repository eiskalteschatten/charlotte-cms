{
  "name": "charlotte-cms",
  "version": "0.0.1",
  "description": "",
  "main": "dist/src/main.js",
  "scripts": {
    "prebuild": "webpack",
    "build": "tsc",
    "dev:backend": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --rs --inspect=0.0.0.0:9229 -- src/main.ts",
    "dev:frontend": "cross-env NODE_ENV=development webpack --watch",
    "dev:db:dump": "docker exec charlottecms-db-1 /usr/bin/mysqldump -u tfstorytime --password=tfstorytime tfstorytime > charlottecms.sql",
    "dev:db:restore": "cat charlottecms.sql | docker exec -i charlottecms-db-1 /usr/bin/mysql -u tfstorytime --password=tfstorytime tfstorytime",
    "predev": "cross-env NODE_ENV=development ts-node ./src/bootstrap.ts",
    "dev": "concurrently  \"npm run dev:backend\" \"npm run dev:frontend\"",
    "prestart": "node ./dist/src/bootstrap.js",
    "start": "node ./dist/src/main.js",
    "test": "npm run lint",
    "lint": "eslint \"./**/*.{tsx,ts,js,json}\"",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eiskalteschatten/charlotte-cms.git"
  },
  "author": "Alex Seifert",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eiskalteschatten/charlotte-cms/issues"
  },
  "homepage": "https://github.com/eiskalteschatten/charlotte-cms#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/ejs": "^3.1.2",
    "@types/html-minifier": "^4.0.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.14.1",
    "@types/nodemailer": "^6.4.8",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "eslint": "^8.31.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-promise": "^7.1.0",
    "husky": "^9.0.11",
    "mini-css-extract-plugin": "^2.9.2",
    "sass": "^1.57.1",
    "sass-loader": "^16.0.5",
    "ts-loader": "^9.5.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3",
    "webpack": "^5.99.6",
    "webpack-cli": "^6.0.1",
    "webpack-manifest-plugin": "^5.0.1"
  },
  "dependencies": {
    "@fastify/autoload": "^6.3.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/csrf-protection": "^7.1.0",
    "@fastify/formbody": "^8.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/secure-session": "^8.2.0",
    "@fastify/static": "^8.2.0",
    "@fastify/view": "^11.1.0",
    "bcryptjs": "^3.0.2",
    "bootstrap": "^5.3.0",
    "dotenv": "^17.0.1",
    "ejs": "^3.1.9",
    "fastify": "^5.4.0",
    "html-minifier": "^4.0.0",
    "jsonwebtoken": "^9.0.2",
    "mariadb": "^3.3.1",
    "nodemailer": "^7.0.3",
    "pino": "^9.3.1",
    "pino-pretty": "^13.0.0",
    "sequelize": "^6.37.3",
    "sequelize-migration-wrapper": "^1.0.1",
    "sequelize-typescript": "^2.1.6"
  }
}
